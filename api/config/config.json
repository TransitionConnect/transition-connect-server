{
  "express": {
    "view engine": null
  },
  "view engines": null,
  "databaseConfig": {
    "host": "bolt://localhost:7689"
  },
  "emailConfig": {
    "host": "localhost",
    "port": 6379,
    "prefix": "q",
    "delay": 10,
    "smtp": {
      "host": "localhost",
      "port": 587,
      "secure": false,
      "auth": {
        "user": "info@localhost.localdomain",
        "pass": "susi"
      }
    }
  },
  "middleware": {
    "compress": {
      "enabled": true
    },
    "session": {
      "enabled": false
    },
    "json": {
      "priority": 30,
      "module": {
        "name": "body-parser",
        "method": "json",
        "arguments": [
          {
            "type": "application/json"
          }
        ]
      }
    },
    "urlEncoded": {
      "priority": 40,
      "module": {
        "name": "body-parser",
        "method": "urlencoded",
        "arguments": [
          {
            "extended": true
          }
        ]
      }
    },
    "appsec": {
      "enabled": true,
      "priority": 90,
      "module": {
        "name": "lusca",
        "arguments": [
          {
            "csrf": false,
            "xframe": "SAMEORIGIN",
            "xssProtection": true,
            "nosniff": true
          }
        ]
      }
    },
    "router": {
      "priority": 100,
      "module": {
        "name": "express-enrouten",
        "arguments": [
          {
            "directory": "path:./src/controller"
          }
        ]
      }
    },
    "errorHandling": {
      "enabled": true,
      "priority": 1000,
      "module": {
        "name": "server-lib/lib/error/errors",
        "method": "handlingError"
      }
    }
  }
}
