{
  "name": "client",
  "version": "0.0.1",
  "description": "Vue.js client source code of the transition connect management gui.",
  "author": "Roger Waldvogel <info@elyoos.org>",
  "private": true,
  "scripts": {
    "watchify-public": "watchify -v -p browserify-hmr -e src/public/main.js -o ../server/static/lib/dist/publicDev.js",
    "watchify-admin": "watchify -v -p [browserify-hmr -p 8888 -u localhost:8888] -e src/admin/main.js -o ../server/static/lib/dist/adminDev.js",
    "watchify": "concurrently \"npm run watchify-public\" \"npm run watchify-admin\"",
    "test": "karma start karma.conf.js",
    "generate-icons": "vsvg -s ./static/svg/icons -t ./src/icons",
    "build-public": "cross-env NODE_ENV=production browserify --exclude vue --exclude vee-validate --exclude vue-router --exclude vuex  --exclude axios --exclude moment --exclude babel-polyfill -g envify -p [ vueify/plugins/extract-css -o ../server/static/styles/public.css ] -e src/public/main.js | uglifyjs -c warnings=false -m > ../server/static/lib/dist/public.min.js",
    "build-admin": "cross-env NODE_ENV=production browserify --exclude vue --exclude vee-validate --exclude vue-router --exclude vuex  --exclude axios --exclude moment --exclude babel-polyfill -g envify -p [ vueify/plugins/extract-css -o ../server/static/styles/admin.css ] -e src/admin/main.js | uglifyjs -c warnings=false -m > ../server/static/lib/dist/admin.min.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js",
    "vee-validate": "vee-validate/dist/vee-validate.js",
    "vue-router": "vue-router/dist/vue-router.js",
    "vuex": "vuex/dist/vuex.js",
    "axios": "axios/dist/axios.js",
    "moment": "moment/min/moment-with-locales.js",
    "babel-polyfill": "babel-polyfill/dist/polyfill.min.js",
    "vue-svgicon": "vue-svgicon/dist/index.js",
    "vue-bootstrap-toggle": "vue-bootstrap-toggle/dist/index.js"
  },
  "dependencies": {
    "vue": "2.3.4",
    "vee-validate": "2.0.0-rc.7",
    "vue-router": "2.7.0",
    "vuex": "2.3.1",
    "axios": "0.16.2",
    "moment": "2.18.1",
    "babel-polyfill": "6.26.0",
    "vue-svgicon": "^1.1.10",
    "vue-bootstrap-toggle": "1.1.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.5",
    "cross-env": "^2.0.0",
    "envify": "4.1.0",
    "http-server": "^0.10.0",
    "jasmine-core": "^2.4.1",
    "karma": "^1.2.0",
    "karma-browserify": "^5.1.0",
    "karma-jasmine": "^1.0.2",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-spec-reporter": "0.0.26",
    "npm-run-all": "^2.3.0",
    "phantomjs-prebuilt": "^2.1.3",
    "proxyquireify": "^3.0.1",
    "uglify-js": "^2.5.0",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0",
    "concurrently": "^3.5.0",
    "node-sass": "^4.5.3"
  }
}
