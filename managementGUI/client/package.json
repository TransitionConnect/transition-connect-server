{
  "name": "client",
  "version": "0.0.1",
  "description": "Vue.js client source code of the transition connect management gui.",
  "author": "Roger Waldvogel <info@elyoos.org>",
  "private": true,
  "scripts": {
    "watchify-public": "watchify -v -p browserify-hmr -e src/public/main.js -o ../server/static/lib/dist/publicDev.js",
    "watchify-admin": "watchify -v -p [browserify-hmr -p 8888 -u localhost:8888] -e src/admin/main.js -o ../server/static/lib/dist/adminDev.js",
    "watchify": "concurrently \"npm run watchify-public\" \"npm run watchify-admin\"",
    "test": "karma start karma.conf.js",
    "generate-icons": "vsvg -s ./static/svg/icons -t ./src/icons",
    "build-public": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o ../server/static/styles/public.css ] -e src/public/main.js | uglifyjs -c warnings=false -m > ../server/static/lib/dist/public.min.js",
    "build-admin": "cross-env NODE_ENV=production browserify -g envify -p [ vueify/plugins/extract-css -o ../server/static/styles/admin.css ] -e src/admin/main.js | uglifyjs -c warnings=false -m > ../server/static/lib/dist/admin.min.js"
  },
  "browserify": {
    "transform": [
      "babelify",
      "vueify"
    ]
  },
  "browser": {
    "vue": "vue/dist/vue.common.js",
    "vee-validate": "vee-validate/dist/vee-validate.js",
    "vue-router": "vue-router/dist/vue-router.js",
    "vuex": "vuex/dist/vuex.js",
    "axios": "axios/dist/axios.js",
    "es6-promise": "es6-promise/dist/es6-promise.min.js",
    "moment": "moment/min/moment-with-locales.js",
    "babel-polyfill": "babel-polyfill/dist/polyfill.min.js",
    "vue-svgicon": "vue-svgicon/dist/index.js",
    "deep-equal": "deep-equal/index.js",
    "innersvg-polyfill": "innersvg-polyfill/innersvg.js"
  },
  "dependencies": {
    "axios": "0.16.2",
    "babel-polyfill": "6.26.0",
    "es6-promise": "4.1.1",
    "innersvg-polyfill": "0.0.2",
    "moment": "2.18.1",
    "deep-equal": "1.0.1",
    "vee-validate": "2.0.0-rc.19",
    "vue": "2.5.3",
    "vue-router": "2.7.0",
    "vue-svgicon": "1.1.10",
    "vuex": "2.3.1"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-plugin-transform-runtime": "^6.0.0",
    "babel-preset-es2015": "^6.0.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    "babelify": "^7.2.0",
    "browserify": "^13.1.0",
    "browserify-hmr": "^0.3.5",
    "concurrently": "^3.5.0",
    "cross-env": "^2.0.0",
    "envify": "4.1.0",
    "http-server": "^0.10.0",
    "mocha": "3.5.3",
    "sinon": "3.3.0",
    "chai": "4.1.2",
    "karma": "1.7.1",
    "karma-browserify": "5.1.1",
    "karma-chai": "0.1.0",
    "karma-mocha": "1.3.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon": "1.0.5",
    "karma-junit-reporter": "1.2.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^2.3.0",
    "phantomjs-prebuilt": "^2.1.3",
    "proxyquireify": "^3.0.1",
    "uglify-js": "^2.5.0",
    "vueify": "^9.4.1",
    "watchify": "^3.9.0",
    "matchdep": "2.0.0",
    "grunt": "1.0.1",
    "grunt-karma": "2.0.0",
    "grunt-contrib-clean": "1.1.0"
  }
}
